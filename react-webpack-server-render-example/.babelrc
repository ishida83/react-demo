{
  "presets": [
    "latest",
    "es2015",
    "react",
    "es2017",
    "stage-0",
    "stage-1",
    "stage-3"
  ],
  "plugins": [
    "transform-react-jsx"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-constant-elements"
      ]
    }
  },
  "ignore": [
    "foo.js",
    "bar/**/*.js"
  ]
}